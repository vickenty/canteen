% layout "back", title => "Edit menu for $date";

<form class="form-horizontal" method="POST">
% foreach (0..5) {
    <div class="control-group">
        <input name="name_<%= $_ %>" value="<%= $menu->[$_] %>">
    </div>
% }
<div class="control-group">
    <input type="submit" class="btn btn-primary" value="Save">
</div>
